    "test": "ts-mocha -p ./tsconfig.tests.json -r jsdom-global/register 'tests/**/*.ts'"
  },
  "author": "Leone",
  "repository": "https://github.com/lgirma/boost-web",
  "homepage": "https://github.com/lgirma/boost-web/tree/master/boost-web-forms",
