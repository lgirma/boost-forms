    "react",
    "svelte"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.10",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "mocha": "^8.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.0"
  },
  "dependencies": {
    "boost-web-core": "^1.5.0"
