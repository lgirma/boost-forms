{
  "name": "boost-web-forms",
  "version": "1.3.0",
  "description": "An opinionated, small form generator library for javascript",
  "module": "dist/boost-web-forms.es.js",
  "main": "dist/boost-web-forms.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --project tsconfig.typedec.json",
